{
  "editor.fontFamily": "SF Mono, JetBrains Mono, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.wordWrap": "off",
  "editor.multiCursorLimit": 11000,
  "editor.lineHeight": 1.7,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment", "keyword", "storage", "entity.other.attribute-name"],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },

  "workbench.colorTheme": "Dracula Theme",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": false,
  "workbench.panel.showLabels": false,

  "window.menuBarVisibility": "toggle",

  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,

  "git.autofetch": true,
  "git.enableSmartCommit": true,

  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",

  "terminal.integrated.fontFamily": "MesloLGM Nerd Font",
  "terminal.integrated.env.windows": {},

  "diffEditor.ignoreTrimWhitespace": false,

  "prettier.printWidth": 120,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "all",

  "powermode.shake.enabled": false,
  "powermode.explosions.backgroundMode": "image",
  "powermode.enabled": true,
  "powermode.explosions.customExplosions": ["https://media.giphy.com/media/SV0LuYdSMmKEklpZKs/giphy.gif"],
  "powermode.explosions.duration": 1000,
  "powermode.explosions.size": 8,
  "powermode.explosions.maxExplosions": 1,
  "powermode.explosions.offset": 0.6,
  "powermode.combo.counterEnabled": "hide",
  "powermode.combo.timerEnabled": "hide",

  "vscode_custom_css.imports": ["file:///C:/Users/ADMIN/.vscode/vs-style.css"],

  "reactSnippets.settings.importReactOnTop": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "cSpell.language": "en,vi",
  "cSpell.userWords": ["clsx", "tiktok", "Zalo"],
  "console-ninja.featureSet": "Community",
  "tabnine.experimentalAutoImports": true,
  "eslint.codeActionsOnSave.rules": null,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  }
}
